{
  "manifest_version": 3,
  "name": "Mindat Autofill",
  "version": "1.0",
  "permissions": [
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "*://www.mindat.org/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Load your collection CSV"
  },
  "content_scripts": [
    {
      "matches": ["*://www.mindat.org/catedit.php*"],
      "js": ["content.js"]
    }
  ]
}
